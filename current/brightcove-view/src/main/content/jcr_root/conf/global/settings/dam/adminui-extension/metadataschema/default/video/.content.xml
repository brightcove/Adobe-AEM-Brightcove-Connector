                                        <education
                                                jcr:primaryType="nt:unstructured"
                                                text="Education"
                                                value="Education"/>
                                        <tech
                                                jcr:primaryType="nt:unstructured"
                                                text="Science &amp; Technology"
                                                value="Science &amp; Technology"/>
                                        <nonprofit
                                                jcr:primaryType="nt:unstructured"
                                                text="Nonprofits &amp; Activism"
                                                value="Nonprofits &amp; Activism"/>
                                        <travel
